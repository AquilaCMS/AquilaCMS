{
  "name": "aquila-cms",
  "version": "1.101.06",
  "description": "CMS e-commerce",
  "main": "server.js",
  "license": "OSL-3.0",
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:AquilaCMS/AquilaCMS.git"
  },
  "scripts": {
    "start": "babel-node server.js",
    "start:pm2": "pm2-runtime start --interpreter babel-node ecosystem.config.js",
    "start:dev": "pm2-dev start --interpreter babel-node ecosystem.config.js",
    "start:aws": "yarn install --prod && yarn cache clean && pm2-runtime start server.js --env production --interpreter babel-node",
    "aqstart": "yarn install --prod && yarn cache clean && pm2-runtime start server.js --env production --interpreter babel-node",
    "reload": "pm2 reload ecosystem.config.js --update-env && pm2 restart --interpreter babel-node server && pm2 log server",
    "build:win": "next build .\\themes\\%npm_config_theme%",
    "build:linux": "next build ./themes/${npm_config_theme}",
    "lint:check": "eslint --ext js,jsx .",
    "lint:fix": "eslint --fix --ext js,jsx .",
    "test": "mocha --exit",
    "coverage": "nyc npm run test",
    "husky:install": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.12.7",
