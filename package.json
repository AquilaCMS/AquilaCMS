{
  "name": "aquila-cms",
  "version": "1.209.14",
  "description": "CMS e-commerce",
  "main": "server.js",
  "license": "OSL-3.0",
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:AquilaCMS/AquilaCMS.git"
  },
  "scripts": {
    "start": "node server.js",
    "start:pm2": "pm2-runtime start ecosystem.config.js",
    "start:dev": "pm2-dev start ecosystem.config.js",
    "start:aws": "yarn install --prod && yarn cache clean && pm2-runtime start server.js --env production",
    "aqstart": "yarn install --prod && yarn cache clean && pm2-runtime start server.js --env production",
    "reload": "pm2 reload ecosystem.config.js --update-env && pm2 restart server && pm2 log server",
    "build": "node ./scripts/build",
    "lint:check": "eslint --ext js,jsx .",
    "lint:fix": "eslint --fix --ext js,jsx .",
    "test": "mocha --exit",
    "coverage": "nyc npm run test",
    "husky:install": "husky install"
  },
